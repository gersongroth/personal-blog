{
  "name": "cms-sanity",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3005",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap",
    "sanity": "cd studio && sanity start"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.13.0",
    "@sanity/image-url": "^0.140.22",
    "classnames": "^2.3.1",
    "date-fns": "^2.23.0",
    "next": "^11.0.1",
    "next-sanity": "^0.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.12.0",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@sanity/cli": "^2.13.1",
    "eslint": "^7.31.0",
    "eslint-config-next": "^11.0.1",
    "lerna": "^4.0.0",
    "postcss": "^8.3.6",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  }
}
